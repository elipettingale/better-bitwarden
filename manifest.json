{
    "manifest_version": 3,
    "name": "Better Bitwarden",
    "description": "A simple extension that cleans up the Bitwarden UI to make it more usable.",
    "version": "1.0",
    "action": {
        "default_icon": {
            "16": "icon/16.png",
            "32": "icon/32.png",
            "48": "icon/48.png",
            "128": "icon/128.png"
        }
    },
    "content_scripts": [
        {
          "matches": ["https://vault.bitwarden.com/*"],
          "css": ["assets/css/index.css"]
        }
    ]
}